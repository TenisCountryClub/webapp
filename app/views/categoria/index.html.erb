<p id="notice"><%= notice %></p>

<h1>Categoria</h1>

<table>
  <thead>
    <tr>
      <th>Torneo</th>
      <th>Nombre</th>
      <th>Numero jugadores</th>
      <th>Numero grupos</th>
      <th>Numero jugadores grupo</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categoria.each do |categorium| %>
      <tr>
        <td><%= categorium.torneo_id %></td>
        <td><%= categorium.nombre %></td>
        <td><%= categorium.numero_jugadores %></td>
        <td><%= categorium.numero_grupos %></td>
        <td><%= categorium.numero_jugadores_grupo %></td>
        <td><%= categorium.tipo %></td>
        <td><%= link_to 'Show', [@torneo,categorium] %></td>
        <td><%= link_to 'Edit', edit_torneo_categorium_path(@torneo,categorium) %></td>
        <td><%= link_to 'Destroy', [@torneo,categorium], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categorium', new_torneo_categorium_path(@torneo) %>
