<p id="notice"><%= notice %></p>

<h1>Partidos</h1>

<table>
  <thead>
    <tr>
      <th>Jugador uno</th>
      <th>Jugador dos</th>
      <th>Cancha</th>
      <th>Hora inicio</th>
      <th>Hora fin</th>
      <th>Grupo</th>
      <th>Cuadro</th>
      <th>Ronda</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partidos.each do |partido| %>
      <tr>
        <td><%= partido.jugador_uno_id %></td>
        <td><%= partido.jugador_dos_id %></td>
        <td><%= partido.numero_cancha %></td>
        <td><%= partido.hora_inicio %></td>
        <td><%= partido.hora_fin %></td>
        <td><%= partido.grupo_id %></td>
        <td><%= partido.cuadro_id %></td>
        <td><%= partido.ronda %></td>
        <td><%= link_to 'Show', partido %></td>
        <td><%= link_to 'Edit', edit_partido_path(partido) %></td>
        <td><%= link_to 'Destroy', partido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partido', new_partido_path %>
