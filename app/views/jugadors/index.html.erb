<p id="notice"><%= notice %></p>

<h1>Jugadors</h1>

<table>
  <thead>
    <tr>
      <th>Numero</th>
      <th>Nombre</th>
      <th>Ranking</th>
      <th>Edad</th>
      <th>Club asociacion</th>
      <th>Fecha inscripcion</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jugadors.each do |jugador| %>
      <tr>
        <td><%= jugador.numero %></td>
        <td><%= jugador.nombre %></td>
        <td><%= jugador.ranking %></td>
        <td><%= jugador.edad %></td>
        <td><%= jugador.club_asociacion %></td>
        <td><%= jugador.fecha_inscripcion %></td>
        <td><%= jugador.status %></td>
        <td><%= link_to 'Show', jugador %></td>
        <td><%= link_to 'Edit', edit_jugador_path(jugador) %></td>
        <td><%= link_to 'Destroy', jugador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jugador', new_jugador_path %>
